;;
;;  Key definitions for the EDT emulation within GNU Emacs
;;

(defconst *EDT-keys*
  '(
    
;;
;;  Arrows
;;
    ("UP" . [up])
    ("DOWN" . [down])
    ("LEFT" . [left])
    ("RIGHT" . [right])

;;
;;  PF keys
;;
    ("PF1" . "")
    ("PF2" . [kp-divide])
    ("PF3" . [kp-multiply])
    ("PF4" . [kp-subtract])

;;
;;  KP0-9 KP- KP, KPP and KPE
;;
    ("KP0" . "0")
    ("KP1" . "1")
    ("KP2" . "2")
    ("KP3" . "3")
    ("KP4" . "4")
    ("KP5" . "5")
    ("KP6" . "6")
    ("KP7" . "7")
    ("KP8" . "8")
    ("KP9" . "9")
    ("KP-" . [kp-add])
    ("KP," . "" )
    ("KPP" . ".")
    ("KPE" . [kp-enter])

;;
;;  Editing keypad (FIND, INSERT, REMOVE)
;;                 (SELECT, PREVIOUS, NEXT)
;;
    ("FIND" . "" )
    ("INSERT" . "" )
    ("REMOVE" . "" )
    ("SELECT" . "" )
    ("PREVIOUS" . "" )
    ("NEXT" . "" )

;;
;;  F1-14 Help Do F17-F20
;;
    ("F1" . [f1])
    ("F2" . [f2])
    ("F3" . [f3])
    ("F4" . [f4])
    ("F5" . [f5])
    ("F6" . [f6])
    ("F7" . [f7])
    ("F8" . [f8])
    ("F9" . [f9])
    ("F10" . [f10])
    ("F11" . "" )
    ("F12" . [f12])
    ("F13" . "" )
    ("F14" . "" )
    ("HELP" . "" )
    ("DO" . "" )
    ("F17" . "" )
    ("F18" . "" )
    ("F19" . "" )
    ("F20" . "" )
;;
;;  Extra Keys
;;
    )
  )
